<div class="col-md-6 col-md-offset-3"
	<div class="user-list">
		<h2>User List (<%= @users.count %>)
	  	<div class="pull-right"><%= button_to "Add New User", new_user_path, class: "btn btn-default", :method => :get %></div>
	  </h2>
		<div class="list-group">
			<% @users.each do |user| %>
				<%= link_to "#{user.name} (#{@userStats[user.id][1]}W #{@userStats[user.id][0]}L)", user_path(user.id), { class: "list-group-item" } %>
			<% end %>
		</div>
		<%= link_to "Add New Player", new_user_path, {class: "btn btn-default"} %>
	</div>
</div>