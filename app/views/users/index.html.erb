<div class="user-list">
	<h2>User List (<%= @users.count %>)</h2>
	<div class="list-group">
		<% @users.each do |user| %>
			<%= link_to "#{user.name} (#{@userStats[user.id][1]}W #{@userStats[user.id][0]}L)", user_path(user.id), { class: "list-group-item" } %>
		<% end %>
	</div>
	<%= link_to "Add New Player", new_user_path, {class: "btn btn-default"} %>
</div>
